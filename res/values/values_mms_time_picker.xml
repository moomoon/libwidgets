<resources>

    <color name="time_picker_stripe">#ff888888</color>
    <color name="time_picker_stripe_unavailable">#ffdddddd</color>
    <color name="time_picker_stripe_pressed">#ff5289dc</color>
    <color name="time_picker_line">#ff777777</color>
    <color name="time_picker_text">#ff777777</color>
    <color name="time_picker_stripe_selected">#88ffffff</color>
    <color name="time_picker_extra_bt_text">@android:color/black</color>
    <color name="time_picker_bg">@android:color/white</color>
    <color name="time_picker_title_text">#ff000000</color>
    <color name="time_keyboard_text_normal">#ff5a5a5a</color>
    <color name="time_keyboard_text_pressed">@android:color/black</color>
    <color name="time_keyboard_text_disabled">#ffdddddd</color>
    <color name="time_keyboard_bg_normal">@android:color/transparent</color>
    <color name="time_keyboard_bg_disabled">@android:color/transparent</color>
    <color name="time_keyboard_bg_pressed">@android:color/white</color>
    <color name="time_keyboard_bg">@android:color/white</color>

    <dimen name="time_picker_line_long">20dp</dimen>
    <dimen name="time_picker_line_short">10dp</dimen>
    <dimen name="time_picker_number_large">15sp</dimen>
    <dimen name="time_picker_number_small">10sp</dimen>
    <dimen name="time_picker_text_padding_right">2dp</dimen>
    <dimen name="time_picker_text_padding_bottom">2dp</dimen>
    <dimen name="time_picker_slider_padding_vertical">15dp</dimen>
    <dimen name="time_picker_slider_height">60dp</dimen>
    <dimen name="time_picker_extra_bt_text">13dp</dimen>
    <dimen name="time_picker_extra_bt_margin_left">1dp</dimen>
    <dimen name="time_picker_extra_bt_margin_top">1dp</dimen>
    <dimen name="time_picker_extra_bt_margin_right">1dp</dimen>
    <dimen name="time_picker_extra_bt_margin_bottom">1dp</dimen>
    <dimen name="time_keyboard_button_height">30dp</dimen>
    <dimen name="time_keyboard_button_text">20dp</dimen>
    <dimen name="time_keyboard_button_text_small">15dp</dimen>

    <string name="time_picker_time_seperator">:</string>

    <style name="time_keyboard_button">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/time_keyboard_bg</item>
        <item name="android:textColor">@drawable/time_keyboard_bg</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="time_keyboard_button_big" parent="@style/time_keyboard_button">
        <item name="android:textSize">@dimen/time_keyboard_button_text</item>
    </style>

    <style name="time_keyboard_button_small" parent="@style/time_keyboard_button">
        <item name="android:textSize">@dimen/time_keyboard_button_text_small</item>
    </style>

</resources>